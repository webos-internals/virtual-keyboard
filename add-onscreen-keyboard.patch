+/* oskb */
+updateList:function(o,m){
+  var keyDesc;
+  var defKeyDesc;
+  var display;
+  var index;
+  var data = {};
+  var that=this;
+  var orientation = o;
+  var mode = m;
+  var themeObject=this.themeObject||this.localizedTableFull;
+
+  if (orientation&&mode) {
+    themeObject.each(function(t){
+      index=t.index;
+      if ((index>=0)&&(index<that.charList.length)){
+        data=that.charList[index];
+        data.output=that.getKeyAttribute(t,index,orientation,mode,'output',that.charList);
+        data.span=that.getKeyAttribute(t,index,orientation,mode,'span',that.charList);
+        data.keyCode=that.getKeyAttribute(t,index,orientation,mode,'keyCode',that.charList);
+        data.display=that.getKeyAttribute(t,index,orientation,mode,'display',that.charList);
+        data.keyhtml=data.display&&(that.makeImg(data.display.img,data.output||" ",index)||data.display.html)||data.output;
+        data.background=that.getKeyAttribute(t,index,orientation,mode,'background',that.charList);
+        data.opacity=that.getKeyAttribute(t,index,orientation,mode,'opacity',that.charList);
+        that.charList[index]=data;
+        that.charDivs[index].firstDescendant().innerHTML=data.keyhtml;
+        that.charDivs[index].setStyle({"background":data.background||""});
+        that.charDivs[index].setOpacity(data.opacity);
+        if (data.display&&data.display.img)
+          that.charDivs[index].removeClassName("kb-text");
+        else
+          that.charDivs[index].addClassName("kb-text");
+      }
+    });
+  }
+
+  var newWidth;
+  this.charList.each(function(c){
+    that.maybeSetDiv(c.index,c.keyCode);
+    if (c.span) {
+      newWidth = c.span * 10 + "%";
+    }
+    else {
+      newWidth="10%";
+    }
+    that._getMatching(that.charPicker,c.index).setStyle({"width":newWidth});
+  });
+  console.log("list updated");
+},
