+  if (keyCode === Mojo.Char.sym) {
+    this.symIdx = index;
+  }
+  if (keyCode === 0) {
+    this.dragIdx = index;
+  }
+  if (keyCode === 1) {
+    this.themeIdx = index;
+  }
